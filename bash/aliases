alias tmprestart="touch tmp/restart.txt"
alias ls="ls -hF"
alias less="less -r"
alias apachectl="sudo /usr/sbin/apachectl"
alias gitcheat="echo -e \"\\nDelete a remote branch:\\n  git push origin :heads/{branch_name}\\n\\nAbandon changes (e.g. bad merge):\\n  git reset --hard\\n\\nUndo last commit\\n  git reset --soft HEAD^\\n\\nRedo last commit including any changes you've staged since\\n  git commit --amend\\n\\nPrune deleted remote branches\\n  git remote prune origin\\n\""
alias mylog="git log --author bill --no-merges"